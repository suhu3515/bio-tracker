<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biotracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BioTracker">
        <activity android:name=".OrderDetailsActivity"></activity>
        <activity android:name=".ViewOrdersActivity" />
        <activity android:name=".ConfirmOrderActivity" />
        <activity android:name=".AddOrderInfoActivity" />
        <activity android:name=".SellerDetailsActivity" />
        <activity android:name=".ProductDetailsActivity" />
        <activity android:name=".GraphActivity" />
        <activity android:name=".ReportsActivity" />
        <activity android:name=".DailyDataActivity" />
        <activity android:name=".FarmDetailsActivity" />
        <activity android:name=".VolumeCalculationActivity" />
        <activity android:name=".FarmActivity" />
        <activity android:name=".AddFarmActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".AddAddressActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ChooseLanguageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>